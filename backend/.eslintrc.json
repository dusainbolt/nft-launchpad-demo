{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "es6": true
  },
  "extends": [
      "eslint:recommended",
      "prettier",
      "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "parserOptions": {
      "ecmaVersion": 12
  },
  "rules": {
      "no-unused-vars": 2,
      "no-use-before-define": 0,
      "curly": 0,
      "no-console": 0,
      "no-empty": 0,
      "arrow-parens": 2,
      "no-return-await": 1,
      "no-var": 2,
      "prefer-const": 2,
      "no-undef": 0,
      "no-param-reassign": 0
  }
}
